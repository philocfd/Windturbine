FoamFile
{
version     2.0;
    format      ascii;
    class       dictionary;
    object      refineMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

set c3;

coordinateSystem global;

globalCoeffs
{
    e1 (1 0 0);
    e2 (0 1 0);
}

directions
(
    e1
    e2
    //e3	//
);

useHexTopology  false;

geometricCut    true;

writeMesh       false;

